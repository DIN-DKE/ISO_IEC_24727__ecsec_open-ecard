			<!-- DIDInfo for Asymmetric Internal Authentication protocol for authentication app-->
			<iso:DIDInfo>
				<iso:DifferentialIdentity>
					<iso:DIDName>Authenticate-rsaSSA-PSS</iso:DIDName>
					<iso:DIDProtocol>urn:oid:1.3.162.15480.3.0.25</iso:DIDProtocol><!--Generic Cryptography-->
					<iso:DIDMarker>
						<iso:CryptoMarker Protocol="urn:oid:1.3.162.15480.3.0.25"><!--for digital signature operation - Asymmetric internal authentication-->
							<iso:AlgorithmInfo>
								<iso:Algorithm>rsaClientAuthentication</iso:Algorithm>
								<iso:AlgorithmIdentifier>
								<!-- pkcs-1 OBJECT IDENTIFIER ::= { iso(1) member-body(2) us(840) rsadsi(113549) pkcs(1) 1
									id-RSASSA-PSS OBJECT IDENTIFIER ::= { pkcs-1 10 } -->
								<iso:Algorithm>http://www.w3.org/2007/05/xmldsig-more#sha256-rsa-MGF1</iso:Algorithm><!--taken from CardInfo_eGK_1-0-0-->
							    </iso:AlgorithmIdentifier>
								<iso:SupportedOperations>Compute-signature</iso:SupportedOperations>
								<iso:CardAlgRef>01</iso:CardAlgRef><!--???-->
							</iso:AlgorithmInfo>
							<iso:KeyInfo>
								<iso:KeyRef>
									<iso:KeyRef>81</iso:KeyRef><!--PrK Reference local first-->
								</iso:KeyRef>
								<iso:KeySize>2048</iso:KeySize>
							</iso:KeyInfo>
							<iso:LegacySignatureGenerationInfo>
								<iso:CardCommand>
									<iso:HeaderTemplate>002241B6</iso:HeaderTemplate><!--MSE-->
									<!--<iso:DataTemplate>{tlv algorithmIdentifier}{tlv keyReference}</iso:DataTemplate>-->
									<iso:DataTemplate>{tlv 0x80 0x91}{tlv 0x84 keyReference}</iso:DataTemplate>
								</iso:CardCommand>
								<iso:CardCommand>
									<iso:HeaderTemplate>002A9E9A</iso:HeaderTemplate><!--PSO(CDS)-->
									<iso:DataTemplate>{hashToSign}</iso:DataTemplate>
									<iso:ExpectedLength>256</iso:ExpectedLength>
								</iso:CardCommand>
							</iso:LegacySignatureGenerationInfo>
							<iso:CertificateRef>
								<iso:DataSetName>AuthenticationCert</iso:DataSetName>
							</iso:CertificateRef>
						</iso:CryptoMarker>
					</iso:DIDMarker>
					<iso:DIDScope>local</iso:DIDScope>
				</iso:DifferentialIdentity>
				<iso:DIDACL>
					<iso:AccessRule>
